    "def read_vowel():\n",
    "    f=open(\"content.txt\",'r')\n",
    "    f1=f.read()\n",
    "    f1=f1.split()\n",
    "    count=0\n",
    "    for a in f1:\n",
    "        if a[0] in 'AEIOUaeiou':\n",
    "            count=count+1\n",
    "    print(\"Total words starting with vowel are \",count)\n",
    "    f.close()\n",
    "read_vowel()"
