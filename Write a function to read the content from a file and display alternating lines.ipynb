    "def alterline():\n",
    "    f=open(\"content.txt\",'r')\n",
    "    f1=f.read()\n",
    "    f1=f1.split()\n",
    "    for a in range(1,len(f1)):\n",
    "        if f1[a-1]=='.':\n",
    "            print(f[a])\n",
    "            while f1[a][0]!='.':\n",
    "                print(f1[a])\n",
    "alterline()\n",
    "            "
