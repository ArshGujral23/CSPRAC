    "def lsearch(lst,ele):\n",
    "    c=0\n",
    "    for a in range(len(lst)):\n",
    "        if lst[a]==ele:\n",
    "            print(\"Element found at\",a+1,'position.')\n",
    "            c=1\n",
    "    if c==0:\n",
    "        print(\"-1\")\n",
    "l=eval(input(\"Enter list: \"))\n",
    "s=int(input(\"Enter element to be searched: \"))\n",
    "lsearch(l,s)"
