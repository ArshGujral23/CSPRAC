    "def shiftelementbehind(l):\n",
    "    a=l[0]\n",
    "    for i in range(1,len(l)):\n",
    "        l[i-1]=l[i]\n",
    "    l[-1]=a\n",
    "    return l\n",
    "\n",
    "n=eval(input(\"Enter a list:\"))\n",
    "print(shiftelementbehind(n))"
   ]
